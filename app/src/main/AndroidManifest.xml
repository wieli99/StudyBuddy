<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wieliFritz.studybuddy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.wieliFritz.studybuddy.HallOfFame"></activity>
        <activity android:name="com.wieliFritz.studybuddy.Pots" />
        <activity android:name="com.wieliFritz.studybuddy.Backgrounds" />
        <activity android:name="com.wieliFritz.studybuddy.Stats" />
        <activity android:name="com.wieliFritz.studybuddy.Settings" />
        <activity android:name="com.wieliFritz.studybuddy.ShopBackgrounds" />
        <activity android:name="com.wieliFritz.studybuddy.Shop" />
        <activity android:name="com.wieliFritz.studybuddy.ShopPots" />
        <activity android:name="com.wieliFritz.studybuddy.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>